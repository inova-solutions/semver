{
  "name": "inova",
  "version": "1.0.0-beta.3",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'packages/semver/**/*.ts' --exec npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts next-version --debug --workspace nx",
    "local-build": "nx build semver && @powershell copy .npmrc dist/packages/semver/.npmrc",
    "next-version": "npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts next-version --debug --workspace nx",
    "bump": "npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts bump --debug --workspace nx"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nrwl/cli": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/conventional-changelog-preset-loader": "^2.3.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.0.3",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "~4.3.5"
  }
}