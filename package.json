{
  "name": "inova",
  "version": "1.0.0-beta.23",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'packages/semver/**/*.ts' --exec npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts last-version --debug --workspace nx",
    "local-build": "nx build semver && @powershell copy .npmrc dist/packages/semver/.npmrc",
    "next-version": "npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts next-version --debug --workspace nx",
    "bump": "npx ts-node --project ./packages/semver/tsconfig.lib.json packages/semver/cli/semver.ts bump --debug --workspace nx",
    "update": "nx migrate latest"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nrwl/cli": "^13.9.2",
    "@nrwl/eslint-plugin-nx": "13.9.2",
    "@nrwl/jest": "13.9.2",
    "@nrwl/js": "13.9.2",
    "@nrwl/linter": "13.9.2",
    "@nrwl/nx-cloud": "13.1.6",
    "@nrwl/workspace": "13.9.2",
    "@types/conventional-changelog-preset-loader": "^2.3.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "nodemon": "^2.0.13",
    "nx": "13.9.2",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "ts-node": "^9.1.1",
    "typescript": "~4.3.5"
  }
}